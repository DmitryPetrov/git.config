[user]
	name = Dmitry Petrov
	email = dmitrypetrov93gmail.com
[color]
	ui = true
[alias]
	lg = log --graph --all --format=format:'%C(bold blue)%h%C(reset) — %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(reset)%C(bold yellow)%d%C(reset)' --abbrev-commit --date=relative
	lgan = log --graph --all --format=format:'%C(bold blue)%h%C(reset) — %C(bold green)(%ar)%C(reset) %C(cyan)%an %C(white)%s%C(reset)%C(reset)%C(bold yellow)%d%C(reset)' --abbrev-commit --date=relative
	lgt = log --graph --all --format=format:'%C(bold blue)%h%C(reset) — %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n'' %C(white)%s%C(reset) %C(bold white)— %an%C(reset)' --abbrev-commit
	hist = log --pretty=format:\"%C(bold blue)%h %C(bold green)%ad %C(reset)| %s%d\" --graph --date=short
	histan = log --pretty=format:\"%C(bold blue)%h %C(bold green)%ad %C(cyan)[%an] %C(reset)| %s%d\" --graph --date=short